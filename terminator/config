[global_config]
  window_state = maximise
  suppress_multiple_term_dialog = True
[keybindings]
  split_horiz = <Primary><Shift>q
  close_window = <Primary><Shift><Alt>F12
  edit_window_title = <Primary><Shift><Alt>w
  ctrl+Left = none
  ctrl+Right = none
  ctrl+Up = none
  ctrl+Down = none
[profiles]
  [[default]]
    background_darkness = 0.8
    background_type = transparent
    cursor_shape = underline
    font = MesloLGS NF 11
    foreground_color = "#00ff00"
    show_titlebar = False
    scrollbar_position = hidden
    scrollback_lines = 10000
    use_custom_command = True
    custom_command = tmux attach -t base || tmux new -s base
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
